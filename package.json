{
  "name": "clipboard-electron-app",
  "version": "v1.6.3-beta",
  "author": "Shelton Zhu",
  "description": "My Electron clipboard app",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/SheltonZhu/clipboard-electron.git"
  },
  "productName": "Electron Clipboard",
  "license": "MIT",
  "keywords": [
    "electron",
    "vue",
    "clipboard"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:build:dev": "vue-cli-service electron:build --mode development",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "ajv": "^7.0.3",
    "appdirectory": "^0.1.0",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.3",
    "electron-cfg": "^1.2.3",
    "electron-clipboard-extended": "^1.1.1",
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
    "electron-vue": "^0.0.1",
    "element-ui": "^2.3.6",
    "md5": "^2.3.0",
    "nedb-promises": "^4.1.0",
    "node-window-manager": "^2.2.4",
    "robotjs": "^0.6.0",
    "vue": "^2.6.11",
    "vue-contextmenujs": "^1.3.13",
    "vue-electron": "^1.0.6",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^11.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5",
    "vue-cli-plugin-element-ui": "~1.1.4",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
